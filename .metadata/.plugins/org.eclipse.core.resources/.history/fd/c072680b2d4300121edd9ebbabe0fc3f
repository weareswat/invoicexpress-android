package pt.rupeal.invoicexpress.dtos;

public class ContactDetailsDTO {

	private String name;
	private String preferencialName;
	private String preferencialPhone;
	private String preferencialEmail;
	
	private String address;
	private String phone;
	private String fax;
	private String email;
	private String nif;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getPreferencialName() {
		return preferencialName;
	}
	public void setPreferencialName(String preferencialName) {
		this.preferencialName = preferencialName;
	}
	public String getPreferencialPhone() {
		return preferencialPhone;
	}
	public void setPreferencialMobilePhone(String preferencialPhone) {
		this.preferencialPhone = preferencialPhone;
	}
	public String getPreferencialEmail() {
		return preferencialEmail;
	}
	public void setPreferencialEmail(String preferencialEmail) {
		this.preferencialEmail = preferencialEmail;
	}
	public String getAddress() {
		return address;
	}
	public void setAddress(String address) {
		this.address = address;
	}
	public String getPhone() {
		return phone;
	}
	public void setMobilePhone(String phone) {
		this.phone = phone;
	}
	public String getFax() {
		return fax;
	}
	public void setFax(String fax) {
		this.fax = fax;
	}
	public String getEmail() {
		return email;
	}
	public void setEmail(String email) {
		this.email = email;
	}
	public String getNif() {
		return nif;
	}
	public void setNif(String nif) {
		this.nif = nif;
	}
	
}
